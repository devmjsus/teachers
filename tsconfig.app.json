{
    "extends": "@vue/tsconfig/tsconfig.dom.json",
    "include": [
        "env.d.ts",
        "src/**/*.ts",
        "src/**/*.js",
        "src/**/*.vue"
    ],
    "exclude": [
        "src/**/__tests__/*",
        "src/components/adadas.vue"
    ],
    "compilerOptions": {
        "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
        "paths": {
            "@/*": ["./src/*"]
        }
    }
}
